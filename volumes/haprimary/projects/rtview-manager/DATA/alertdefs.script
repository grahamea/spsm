CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE ALERTLEVELS(ALERTNAME VARCHAR NOT NULL,INDEXTYPE VARCHAR,ALERTINDEX VARCHAR NOT NULL,WARNINGLEVEL DOUBLE,ALARMLEVEL DOUBLE,DURATION INTEGER,ENABLED BOOLEAN,USEINDEX BOOLEAN,PRIMARY KEY(ALERTNAME,ALERTINDEX))
CREATE MEMORY TABLE AUDIT_TABLE(TIME_STAMP TIMESTAMP,USER VARCHAR,ACTION VARCHAR,ALERTNAME VARCHAR,INDEXTYPE VARCHAR,ALERTINDEX VARCHAR,WARNINGLEVEL DOUBLE,ALARMLEVEL DOUBLE,DURATION INTEGER,ENABLED BOOLEAN,USEINDEX BOOLEAN)
CREATE MEMORY TABLE ACTION_AUDIT_TABLE(TIME_STAMP TIMESTAMP,USER VARCHAR,ACTION_TYPE VARCHAR,ACTION VARCHAR,TARGET VARCHAR,VALUE VARCHAR,ALERT_NAME VARCHAR,ALERT_INDEX VARCHAR)
CREATE MEMORY TABLE "ALERT_PERSIST_TABLE"("AlertEngineName" VARCHAR NOT NULL,"Time" TIMESTAMP,"Alert Name" VARCHAR,"Alert Index" VARCHAR,"Severity" INTEGER,"Alert Text" VARCHAR,"Cleared" BOOLEAN,"Acknowledged" BOOLEAN,"ID" BIGINT NOT NULL,"Last Update Time" TIMESTAMP,"Comments" VARCHAR,"Owner" VARCHAR,"Cleared Reason" VARCHAR,"Cleared Time" TIMESTAMP,"Count" INTEGER,"Alert Index Values" VARCHAR,"Row Update Time" TIMESTAMP,"Source" VARCHAR,"Package" VARCHAR,"Category" VARCHAR,"DrillDownSuffix" VARCHAR,"DrillUpSuffix" VARCHAR,"CacheMetric" VARCHAR,"current" VARCHAR,"level" INTEGER,PRIMARY KEY("AlertEngineName","ID"))
